      <Button layoutX="159.0" layoutY="235.0" mnemonicParsing="false" onAction="#launchQuantumSolver" prefHeight="26.0" prefWidth="281.0" text="Solve Quantum Logic Circuits" />
      <Button fx:id="exitBtn" layoutX="476.0" layoutY="332.0" mnemonicParsing="false" onAction="#exit" prefHeight="26.0" prefWidth="56.0" text="Exit" />
